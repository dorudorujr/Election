<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.g015c1308.election2.LargeRegion">

    <!-- TODO: Update blank fragment layout -->

    <android.support.constraint.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <ImageView
            android:id="@+id/imageView"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:scaleType="fitCenter"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.0"
            app:srcCompat="@drawable/d6636a1bc28adac12dd413221bf6d1af" />

        <Button
            android:id="@+id/hokaidou"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="46dp"
            android:layout_marginRight="44dp"
            android:layout_marginTop="152dp"
            android:text="北海道"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <Button
            android:id="@+id/touhoku"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginRight="82dp"
            android:layout_marginTop="38dp"
            android:text="東北"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/hokaidou"
            android:layout_marginEnd="82dp" />

        <Button
            android:id="@+id/koushinetuhokuriku"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="82dp"
            android:layout_marginRight="82dp"
            android:layout_marginTop="8dp"
            android:text="北陸・甲信越"
            android:textSize="10sp"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/touhoku" />

        <Button
            android:id="@+id/kantou"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="82dp"
            android:layout_marginRight="82dp"
            android:layout_marginTop="8dp"
            android:text="関東"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/koushinetuhokuriku" />

        <Button
            android:id="@+id/toukai"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginRight="82dp"
            android:layout_marginTop="8dp"
            android:text="東海"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/kantou"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="8dp"
            app:layout_constraintVertical_bias="0.0"
            android:layout_marginEnd="82dp" />

        <Button
            android:id="@+id/tyuugoku"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginRight="8dp"
            android:text="中国"
            app:layout_constraintRight_toLeftOf="@+id/koushinetuhokuriku"
            android:layout_marginEnd="8dp"
            android:layout_marginTop="72dp"
            app:layout_constraintTop_toBottomOf="@+id/hokaidou" />

        <Button
            android:id="@+id/kansai"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginRight="8dp"
            android:layout_marginTop="8dp"
            android:text="関西"
            app:layout_constraintRight_toLeftOf="@+id/kantou"
            app:layout_constraintTop_toBottomOf="@+id/tyuugoku"
            android:layout_marginEnd="8dp" />

        <Button
            android:id="@+id/shikoku"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginRight="8dp"
            android:layout_marginTop="8dp"
            android:text="四国"
            app:layout_constraintRight_toLeftOf="@+id/toukai"
            app:layout_constraintTop_toBottomOf="@+id/kansai"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="8dp"
            app:layout_constraintVertical_bias="0.0"
            android:layout_marginEnd="8dp" />

        <Button
            android:id="@+id/kyusyuu"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="8dp"
            android:text="九州"
            app:layout_constraintLeft_toLeftOf="parent"
            android:layout_marginStart="8dp"
            app:layout_constraintRight_toLeftOf="@+id/shikoku"
            android:layout_marginRight="8dp"
            app:layout_constraintHorizontal_bias="0.571"
            android:layout_marginTop="-26dp"
            app:layout_constraintTop_toBottomOf="@+id/shikoku" />
    </android.support.constraint.ConstraintLayout>
</FrameLayout>
